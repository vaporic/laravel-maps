{
  "name": "vaporic/laravel-maps",
  "description": "Eay - peasy map integration for Laravel.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpgmaps",
    "googlemaps",
    "biostall"
  ],
  "authors": [
    {
      "name": "Hugo Espinosa",
      "email": "vaporic@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "illuminate/support": "^8.0"
  },
  "suggest": {
    "geocoder-php/GeocoderLaravel": "If you need to geocode addresses and don't already have marker coordinates, use this package. It provides geocoding functionality optimized for Laravel."
  },
  "autoload": {
    "psr-4": {
      "Vaporic\\LaravelMaps\\": "src/"
    }
  },
  "require-dev": {
    "orchestra/testbench-browser-kit": "^6.0",
    "orchestra/testbench-dusk": "^6.0",
    "orchestra/testbench": "^6.0",
    "php-coveralls/php-coveralls" : "*",
    "phpmd/phpmd": "^2.7",
    "phpunit/phpunit": "^9.0",
    "sebastian/phpcpd": "^5.0",
    "symfony/thanks": "^1.2"
  },
  "autoload-dev": {
    "psr-4": {
        "Vaporic\\LaravelMaps\\Tests\\": "tests/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Vaporic\\LaravelMaps\\Providers\\Service"
          ],
          "aliases": {
              "Map": "Vaporic\\LaravelMaps\\Facades\\Map"
          }
      }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
